{
  "name": "@flump/web",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "version": "1.21.3",
  "dependencies": {
    "@chakra-ui/react": "2.10.4",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@react-router/node": "7.1.1",
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.47.10",
    "cross-env": "7.0.3",
    "d3-shape": "3.2.0",
    "i18next": "24.2.0",
    "i18next-browser-languagedetector": "8.0.2",
    "i18next-fs-backend": "2.6.0",
    "i18next-http-backend": "3.0.1",
    "isbot": "5.1.19",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.4.0",
    "react-router": "7.1.1",
    "recharts": "2.15.0",
    "remix-i18next": "7.0.1",
    "supabase": "2.2.1",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.6.0",
    "@pandacss/dev": "0.51.1",
    "@react-router/dev": "7.1.1",
    "@react-router/fs-routes": "7.1.1",
    "@react-router/remix-routes-option-adapter": "7.1.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/eslint": "9.6.1",
    "@types/node": "22.10.2",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/eslint-plugin": "1.1.23",
    "@vitest/ui": "2.1.8",
    "autoprefixer": "10.4.20",
    "c8": "10.1.3",
    "dotenv": "16.4.7",
    "esbuild": "0.24.2",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-turbo": "2.3.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-no-relative-import-paths": "1.5.5",
    "eslint-plugin-react": "7.37.3",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-sonarjs": "3.0.1",
    "eslint-plugin-storybook": "0.11.1",
    "eslint-plugin-testing-library": "7.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vitest-globals": "1.5.0",
    "globals": "15.14.0",
    "happy-dom": "16.2.9",
    "msw": "2.7.0",
    "postcss": "8.4.49",
    "remix-flat-routes": "0.6.5",
    "rollup-plugin-visualizer": "5.13.1",
    "tsc-files": "1.1.4",
    "tsconfig-paths": "4.2.0",
    "typescript-coverage-report": "1.0.0",
    "typescript-eslint": "8.19.0",
    "vite": "6.0.6",
    "vite-plugin-checker": "0.8.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8",
    "vitest-github-actions-reporter": "0.11.1",
    "vitest-sonar-reporter": "2.0.0"
  },
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write app",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint --fix --cache .",
    "lint:tsc": "tsc --project tsconfig.json",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server/index.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:types": "supabase gen types typescript --local > ./db_types.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:preview": "pnpx vite preview --outDir ./html",
    "pretest:e2e:run": "pnpm run build",
    "ts-coverage": "typescript-coverage-report",
    "ts-coverage:ui": "vite preview --outDir ./coverage-ts/",
    "typecheck": "react-router typegen && tsc",
    "prepare": "panda codegen && react-router typegen",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write",
    "app/**/*.ts": [
      "typescript-coverage-report",
      "tsc-files --noEmit"
    ],
    "app/**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}": [
      "eslint --cache --fix",
      "vitest --run"
    ]
  },
  "typeCoverage": {
    "atLeast": "100.00",
    "ignoreFiles": [
      "app/**/*.config.{js,ts,jsx,tsx}",
      "app/*.*",
      "*.*",
      "app/**/*.test.{js,ts,jsx,tsx}",
      "styled-system/**/*.*",
      "test/**/*.{js,ts,jsx,tsx}",
      "app/utils/utils.ts",
      "app/**/*.d.ts"
    ]
  }
}
