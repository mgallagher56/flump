{
  "name": "@flump/web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "commit": "git-cz",
    "docker": "docker-compose up -d",
    "format": "prettier --write app",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint --fix --cache .",
    "lint:tsc": "tsc --project tsconfig.json",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server/index.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:types": "supabase gen types typescript --local > ./db_types.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:preview": "pnpx vite preview --outDir ./html",
    "pretest:e2e:run": "pnpm run build",
    "ts-coverage": "typescript-coverage-report",
    "ts-coverage:ui": "pnpx vite preview --outDir ./coverage-ts/",
    "panda:prepare": "panda codegen",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write",
    "**/*.ts": [
      "typescript-coverage-report",
      "tsc-files --noEmit"
    ],
    "**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}": [
      "eslint --cache --fix",
      "vitest --run"
    ]
  },
  "typeCoverage": {
    "atLeast": "100.00",
    "ignoreFiles": [
      "app/**/*.config.{js,ts,jsx,tsx}",
      "app/*.*",
      "*.*",
      "app/**/*.test.{js,ts,jsx,tsx}",
      "styled-system/**/*.*",
      "test/**/*.{js,ts,jsx,tsx}",
      "app/utils/utils.ts",
      "app/**/*.d.ts"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.4",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.5",
    "@isaacs/express-prometheus-middleware": "1.2.1",
    "@remix-run/express": "2.9.2",
    "@remix-run/node": "2.9.2",
    "@remix-run/react": "2.9.2",
    "@remix-run/server-runtime": "2.9.2",
    "@remix-run/testing": "2.9.2",
    "@supabase/auth-helpers-remix": "0.4.0",
    "@supabase/supabase-js": "2.43.2",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "d3-shape": "3.2.0",
    "express": "4.19.2",
    "framer-motion": "11.2.5",
    "i18next": "23.11.5",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-fs-backend": "2.3.1",
    "i18next-http-backend": "2.5.2",
    "isbot": "5.1.6",
    "morgan": "1.10.0",
    "prom-client": "15.1.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "14.1.1",
    "recharts": "2.12.7",
    "remix-i18next": "6.1.0",
    "supabase": "1.167.4",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/plugin-syntax-flow": "7.24.1",
    "@babel/plugin-transform-react-jsx": "7.23.4",
    "@commitlint/config-conventional": "19.2.2",
    "@faker-js/faker": "8.4.1",
    "@pandacss/dev": "0.39.1",
    "@remix-run/dev": "2.9.2",
    "@remix-run/eslint-config": "2.9.2",
    "@testing-library/dom": "10.1.0",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/bcryptjs": "2.4.6",
    "@types/compression": "1.7.5",
    "@types/eslint": "8.56.10",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "autoprefixer": "10.4.19",
    "c8": "9.1.0",
    "cookie": "0.6.0",
    "dotenv": "16.4.5",
    "esbuild": "0.21.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-turbo": "^1.13.3",
    "eslint-plugin-no-relative-import-paths": "1.5.4",
    "eslint-plugin-sonarjs": "0.25.1",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "happy-dom": "14.11.0",
    "install": "0.13.0",
    "msw": "2.3.0",
    "npm-run-all2": "6.2.0",
    "postcss": "8.4.38",
    "rollup-plugin-visualizer": "5.12.0",
    "start-server-and-test": "2.0.3",
    "ts-node": "10.9.2",
    "tsc-files": "1.1.4",
    "tsconfig-paths": "4.2.0",
    "typescript-coverage-report": "1.0.0",
    "vite": "5.2.11",
    "vite-plugin-checker": "0.6.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0",
    "vitest-github-actions-reporter": "0.11.1",
    "vitest-sonar-reporter": "2.0.0",
    "webdriverio": "8.36.1"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "version": "1.18.0"
}
