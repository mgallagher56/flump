{
  "name": "flump-f955",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "commit": "git-cz",
    "docker": "docker-compose up -d",
    "format": "prettier --write ./app",
    "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:staged": "panda codegen && lint-staged",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server/index.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:types": "supabase gen types typescript --local > ./db_types.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:preview": "pnpx vite preview --outDir ./html",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"pnpx cypress open\"",
    "pretest:e2e:run": "pnpm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"pnpx cypress run\"",
    "ts-coverage": "typescript-coverage-report",
    "ts-coverage:ui": "pnpx vite preview --outDir ./coverage-ts/",
    "panda:prepare": "panda codegen",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write",
    "**/*.ts": [
      "typescript-coverage-report",
      "tsc-files --noEmit"
    ],
    "**/**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}": [
      "eslint --cache --fix",
      "vitest --run"
    ]
  },
  "typeCoverage": {
    "atLeast": "100.00",
    "ignoreFiles": [
      "app/**/*.config.{js,ts,jsx,tsx}",
      "cypress/**/*.{js,ts,jsx,tsx}",
      "app/*.*",
      "*.*",
      "app/**/*.test.{js,ts,jsx,tsx}",
      "styled-system/**/*.*",
      "test/**/*.{js,ts,jsx,tsx}",
      "starlight/**/*.*",
      "starlight/.astro/**/*.*",
      "app/utils/utils.ts",
      "app/**/*.d.ts"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data",
    "/starlight"
  ],
